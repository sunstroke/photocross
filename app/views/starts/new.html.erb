<h1>New start</h1>

<%= form_for(@start, :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>
  <%= f.file_field :image, :label => "Картинка"%>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :event_id %><br />
    <%= f.text_field :event_id %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', starts_path %>